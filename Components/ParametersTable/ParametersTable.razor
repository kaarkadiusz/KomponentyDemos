@typeparam T

<table>
    <thead>
        <tr>
            <th>@ParametersTableResources.ParameterName</th>
            <th>@ParametersTableResources.ParameterType</th>
            <th>@ParametersTableResources.DefaultValue</th>
        </tr>
    </thead>
    <tbody>
        @foreach(ParameterToDisplay parameter in Parameters)
        {
            <tr>
                <td>@parameter.Name</td>
                <td title="@(parameter.ParameterType.FullName)">@parameter.Type</td>
                <td>@parameter.Default</td>
            </tr>
        }
    </tbody>
    <tfoot>
        <tr>
            <td colspan="10">@ParametersTableResources.AutoGenerated</td>
        </tr>
    </tfoot>
</table>

@code {

}
